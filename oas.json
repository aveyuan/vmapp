{
  "openapi": "3.0.0",
  "info": {
    "title": "后端服务",
    "description": "提供API服务",
    "contact": {
      "name": "aveyuan",
      "email": "aveyuan@163.com"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://127.0.0.1",
      "description": " Server-1"
    }
  ],
  "paths": {
    "/api/v1/hello": {
      "get": {
        "responses": {
          "200": {
            "description": "返回结果",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/vmapp.api.app0.v1.HelloRes"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ],
        "summary": "获取Hello",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "description": "姓名",
            "schema": {
              "type": "string",
              "description": "姓名"
            }
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "HelloReq": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "姓名"
          }
        }
      },
      "vmapp.api.app0.v1.HelloRes": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "description": "返回内容"
          }
        }
      }
    },
    "securitySchemes": {
      "AuthorizationHeader": {
        "type": "输入你的token"
      }
    }
  },
  "security": [
    {
      "AuthorizationHeader": [
        "read",
        "write"
      ]
    }
  ]
}